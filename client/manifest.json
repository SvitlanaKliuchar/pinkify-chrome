{
    "manifest_version": 3,
    "name": "Pinkify",
    "version": "1.0",
    "description": "Daily quotes and Spotify player",
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    },
    "permissions": [
      "storage",
      "identity",
      "activeTab"
    ],
    "host_permissions": [
      "https://api.spotify.com/"
    ]
  }
  